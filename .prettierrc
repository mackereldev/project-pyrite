{
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 500,
    "bracketSameLine": true,
    "trailingComma": "es5",
    "endOfLine": "crlf",
    "plugins": [
        "prettier-plugin-svelte",
        "prettier-plugin-tailwindcss",
        "prettier-plugin-multiline-arrays"
    ],
    "svelteAllowShorthand": true,
    "multilineArraysWrapThreshold": 3
}